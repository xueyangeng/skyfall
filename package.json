    "cssnano": "^5.0.8",
    "inquirer": "^8.2.0",
    "node-emoji": "^1.11.0",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "postcss-cssnext": "^3.1.1",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "request": "^2.88.2",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
